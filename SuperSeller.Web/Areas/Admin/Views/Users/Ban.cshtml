@model BanUserBindingModel
@{
    ViewData["Title"] = "Ban User";
}
<h2>Ban @TempData["Username"]</h2>

<form method="post" class="form-horizontal">
    @Html.FormGroupFor(model => model.EndDate)
    <div class="form-group">
        <div class="col-sm-10 col-sm-push-2">
            <input type="submit" value="Ban" class="btn btn-danger" />
            <a onclick="history.go(-1)" class="btn btn-default">Cancel</a>
        </div>
    </div>
</form>

