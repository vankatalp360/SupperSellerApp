@model ICollection<UsersConciseViewModel>
@{
    ViewData["Title"] = "All Users";
}

<h2>All Users</h2>
<table class="table table-striped">
    <thead>
        <tr>
            <th>UserName</th>
            <th>Email</th>
            <th>City</th>
            <th>RegisteredDate</th>
            <th>Ads</th>
            <th>Admin?</th>
            <th>Actions</th>
        </tr>
    </thead>
    <tbody>
        @Html.DisplayForModel()
    </tbody>
</table>


